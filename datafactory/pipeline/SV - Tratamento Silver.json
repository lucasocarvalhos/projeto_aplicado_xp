{
	"name": "SV - Tratamento Silver",
	"properties": {
		"activities": [
			{
				"name": "NTB - Executa silver",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": {
						"value": "@concat(\n    pipeline().globalParameters.p_caminho_notebook,\n    pipeline().parameters.p_nome_notebook\n)",
						"type": "Expression"
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_databricks",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"p_nome_notebook": {
				"type": "string"
			},
			"p_chave_entidade": {
				"type": "string"
			}
		},
		"folder": {
			"name": "2. Silver/1. Executa notebook"
		},
		"annotations": []
	}
}